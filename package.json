{
  "name": "api",
  "version": "1.1.0",
  "description": "Repository for https://www.udemy.com/course/tdd-com-mango/ course",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "watch": "tsc -w",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoaoBaggio/node-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JoaoBaggio/node-api/issues"
  },
  "homepage": "https://github.com/JoaoBaggio/node-api#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/is-base64": "^1.1.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/knex": "^0.16.1",
    "@types/node": "^15.3.0",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.26.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "rimraf": "^3.0.2",
    "sucrase": "^3.18.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "is-base64": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.19",
    "nodemon": "^2.0.7",
    "npm": "^7.16.0",
    "objection": "^2.2.15",
    "pg": "^8.6.0",
    "run": "^1.4.0",
    "sequelize": "^6.6.2",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "16.x"
  }
}
